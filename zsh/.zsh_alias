# git
alias gco="git checkout"
alias gb="git branch --format='%(HEAD) %(color:yellow)%(refname:short)%(color:reset) %(if)%(upstream)%(then)%(color:blue)%(upstream:track)%(color:reset) %(end)- %(contents:subject) %(color:green)(%(committerdate:relative)) [%(authorname)]' --sort=-committerdate"

# ls
alias ll="ls -l --color"
alias lla="ls -al --color"

# vim
alias vi="vim"
